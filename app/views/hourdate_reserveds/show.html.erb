<%- model_class = HourdateReserved -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:hora) %>:</strong></dt>
  <dt><strong><%= model_class.human_attribute_name(:study_carrel_id) %>:</strong></dt>
  <dd><%= @hourdate_reserved.hora %></dd>
  <dd><%= @hourdate_reserved.study_carrel_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              hourdate_reserveds_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_hourdate_reserved_path(@hourdate_reserved), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              hourdate_reserved_path(@hourdate_reserved),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
