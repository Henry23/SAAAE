<div class="form-group">
  	<h2>Escanee su carne</h2>
	<%= form_tag home_path, method: "get" do %>
		<div class="col-lg-10">
		  <%= text_field_tag :search, params[:search], :class => 'form-control', :placeholder => 'No. de Cuenta' %>
		</div>
		<div class="col-lg-2">
			<%= submit_tag "Buscar", :class => 'btn btn-primary'%>
		</div>
	<% end %>
</div>


<% @student.each do |stud| %>
  <p> <%= stud.name %></p>
<% end %>
