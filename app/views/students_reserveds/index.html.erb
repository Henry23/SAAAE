<%- model_class = StudentsReserved -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:reservation_id)%></th>
      <th><%= model_class.human_attribute_name(:student_id) %> </th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @students_reserveds.each do |students_reserved| %>
        <tr>
        <td><%= link_to students_reserved.id, students_reserved_path(students_reserved) %></td>
        <td><%= students_reserved.reservation_id %></td>
        <td><%= students_reserved.student_id %></td>
        <td><%= students_reserved.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_students_reserved_path(students_reserved), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      students_reserved_path(students_reserved),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_students_reserved_path,
            :class => 'btn btn-primary' %>
