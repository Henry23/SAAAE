<%- model_class = StudyCarrel -%>

<div class="page-header">
  <h1>Tabla de Horas</h1>
</div>

<dl class="dl-horizontal">

<dl class="dl-horizontal">
  <table class="table table-striped">
  <thead>
    <tr>
      <th> Hora Disponible </th>
      <th> choose </th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <% @study_carrel.hourdate_reserved.each do |t| %>
      <td>  
        <dd> <%= t.hora.strftime("%I")	%>
             <%= t.hora.strftime("%M") %>
             <%= t.hora.strftime("%p")	%>
        </dd>
      </td>
      <td>
        <%= link_to "Escojer", "/path1",:class => 'btn btn-default btn-xs' %>
      </td>
    </tr>
    
    <% end %>
  </tbody>
</table>
