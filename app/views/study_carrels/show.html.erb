<%- model_class = StudyCarrel -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:max_number) %>:</strong></dt>
  <dd><%= @study_carrel.max_number %></dd>
  <dt><strong><%= model_class.human_attribute_name(:code) %>:</strong></dt>
  <dd><%= @study_carrel.code %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              study_carrels_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_study_carrel_path(@study_carrel), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              study_carrel_path(@study_carrel),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'EstÃ¡ seguro?')) },
              :class => 'btn btn-danger' %>
